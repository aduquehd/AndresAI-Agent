{
  "name": "ai-agent",
  "version": "1.0.0",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "dev": "tsc --watch",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "format:html": "prettier --write \"templates/**/*.html\"",
    "format:js": "prettier --write \"static/**/*.js\"",
    "format:css": "prettier --write \"static/**/*.css\""
  },
  "devDependencies": {
    "typescript": "^5.0.0",
    "@types/node": "^20.0.0",
    "prettier": "^3.0.0"
  }
}
